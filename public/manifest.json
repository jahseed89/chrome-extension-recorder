{
  "manifest_version": 3,
  "name": "Screen Recorder",
  "version": "1.0",
  "permissions": ["desktopCapture", "storage", "activeTab", "http://*/*", "https://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo521.png"
  }
}
